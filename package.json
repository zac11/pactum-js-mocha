{
  "name": "pactum-js-mocha",
  "version": "1.0.0",
  "description": "Pactum Tests with Mocha and Mochawesome reporter",
  "main": "index.js",
  "scripts": {
    "test": "mocha ./tests/reqres-api/*.js",
    "test-new": "mocha ./automation-excercise/*.js",
    "test-all": "mocha ./tests/**/*.js",
    "test-reports": "mocha ./reqres-api/*.js --reporter spec --reporter mochawesome --timeout 5000",
    "test-new-reports": "mocha ./automation-excercise/*.js --reporter spec --reporter mochawesome --timeout 5000"
  },
