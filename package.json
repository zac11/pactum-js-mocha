    "@faker-js/faker": "^8.0.2",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "pactum": "^3.4.1"
  },
  "dependencies": {
    "chai": "^4.3.7",
    "find-config": "^1.0.0"
  }
}
